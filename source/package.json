{
  "name": "ambee-graphql-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "prestart": "export $([ -e '.env' ] && grep  -v '^#' .env | xargs)",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "rimraf dist && export $([ -e '.env' ] && grep  -v '^#' .env | xargs) && nest start --watch",
    "start:debug": "export $([ -e '.env' ] && grep  -v '^#' .env | xargs) && nest start --debug --watch",
    "start:prod": "export $([ -e '.env' ] && grep  -v '^#' .env | xargs) && node -r ./tsconfig-paths-bootstrap.js dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^7.3.2",
    "@nestjs/core": "^7.3.2",
    "@nestjs/graphql": "^7.5.5",
    "@nestjs/platform-express": "^7.3.2",
    "apollo-cache-control": "^0.11.1",
    "apollo-datasource-rest": "^0.9.3",
    "apollo-server-cache-redis": "^1.2.2",
    "apollo-server-express": "^2.15.1",
    "apollo-server-plugin-response-cache": "^0.5.3",
    "aws-sdk": "^2.726.0",
    "dotenv": "^8.2.0",
    "graphql": "^15.3.0",
    "graphql-tools": "^6.0.12",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^2.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.0",
    "tsconfig-paths": "^3.9.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.4.1",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.3.2",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.4",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.0.22",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.22.0",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.1",
    "ts-loader": "^8.0.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.9.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
